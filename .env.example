# API Configuration
VITE_API_BASE_URL=https://3-d-point-cloud-labeling-platform-backend-9dozb1j5y.vercel.app/api

# Environment
VITE_NODE_ENV=development  # development, production, test

# Authentication (if using OAuth or other auth providers)
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_FACEBOOK_APP_ID=your_facebook_app_id

# Cloudinary Configuration (if using for image uploads)
VITE_CLOUDINARY_CLOUD_NAME=your_cloud_name
VITE_CLOUDINARY_UPLOAD_PRESET=your_upload_preset

# Feature Flags (if needed)
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_MAINTENANCE_MODE=false

# WebSocket Configuration (if using WebSockets)
VITE_WS_URL=ws://localhost:5000  # WebSocket server URL

# Sentry or Error Tracking (if using)
VITE_SENTRY_DSN=your_sentry_dsn

# Map or Geolocation API Keys (if needed)
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_token
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
